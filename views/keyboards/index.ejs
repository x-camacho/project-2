<%- include ('../partials/navBar.ejs') %>

<div class= "testy">
  <div>
    <div class="list-container-child">
      <h3> Build List:</h3>
      <% keyboards.forEach( k => { %>
        <div class="row card-panel hoverable">
          <div class="col s12 m6">
            <div class="card deep-purple lighten-3">
              <div class="card-content black-text">
                <span class="card-title"><%= k.name %></span>
                <p>
                 Created By USER ID: <%= k.createdBy[0].name %>
                </p>
              </div>
              <div class="card-action">
                <a class="waves-effect waves-light btn black-text blue-grey lighten-3" href='/keyboards/<%= k._id %>'>Details</a>
              </div>
              </div>
            </div>
          </div>
          <% }); %>
        </tbody>
      </table>

<div class="line"></div>
